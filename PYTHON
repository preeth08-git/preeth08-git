#LOOPS     i)for loop

for i in range(0, 500):       #range allows the user to generate a series of number
   print(i)

tuple = (17,"oct","preeth")   
for item in tuple:            #for loop by tuples
   print(item)

list = [16,"jan","tanvi"]
for item in list:             #for loop by list
   print(item)

set2 = {1,"may","akrithi"}
for item in set2:             #for loop by set (remember set gives answer in alphabetic order)
   print(item)

studentdict = {
   "Age" : 41,
   "Name" : "dimple\n",
   "haircolour" : "black\n"    #for loop by dictionary
}
for items in studentdict:
   print(studentdict)


#WHILE LOOP:
i = 0
while(i<100):      #while loop
   i = i +1
   if i == 99:
      continue  # continue funtion doesnt includes the specific number
   print(i)
#break      #break funtion helps to break the statement



