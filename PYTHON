#if STATEMENT
m = 57
z = 9
if m > z :
    print(" m is greater than z")
    print(m)
 
#if elif
z1 = 83
m1 = 83
if z1 > m1:
  print("z1 is greater than m1")
elif z1 == m1:
   
   print("z1 and m1 are both equal")
   print(z1,m1)

#else
z2 = 74
m2 = 953
if m2 < z2 :
   print("m2 is smaller than z2")
elif m2 == z2 :
   print("m2 and z2 are equal")
else:
   
  print("m2 is bigger than z2 ",m2 > z2)
  print(m2,z2)
   
